<AnchorPane prefHeight="572.0" prefWidth="929.0" stylesheets="@../style/LoginPageStyle.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.fisheryharbour.controller.UserVerifyFormController">
   <style>-fx-background-color: white;</style>
   <children>
      <ImageView fitHeight="132.0" fitWidth="166.0" layoutX="163.0" layoutY="141.0" pickOnBounds="true" preserveRatio="true">
