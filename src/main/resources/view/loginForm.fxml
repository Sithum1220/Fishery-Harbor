<AnchorPane prefHeight="572.0" prefWidth="929.0" style="-fx-background-color: white;" stylesheets="@../style/LoginPageStyle.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.fisheryharbour.controller.LoginFormController">
